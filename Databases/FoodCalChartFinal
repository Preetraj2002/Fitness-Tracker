CREATE OR REPLACE TRIGGER calculate_calories
BEFORE INSERT ON FOODLOG
FOR EACH ROW
DECLARE
    food_cal DECIMAL(10, 2);
BEGIN
    SELECT "CAL/100" INTO food_cal
    FROM FoodCalChart
    WHERE FOODNAME = :NEW.Food;

    :NEW.Calories := (:NEW.Quantity / 100) * food_cal;
END;
/
INSERT INTO FOODLOG (UserID, Timestamp, FoodCategory, Food, Quantity)
SELECT 1, '2024-04-01 9:00 AM', 'Sugars', 'Sugarcane, juice', 42 FROM DUAL ;

SELECT *
FROM FOODLOG
WHERE Timestamp = '2024-04-01 9:00 AM';
